void setup()
{
pinMode(3, OUTPUT);
pinMode(10, INPUT);
}
void loop() {
int hasObstacle = digitalRead(10);
if (hasObstacle == 1) {
digitalWrite(3, HIGH);
}
else {
digitalWrite(3, LOW);
}
delay(200);
}




//
//IR Sensor 3pin end goes in 4th from Right of bottom pin line

//2 6 pin ends on to LED, other to counter spot.


